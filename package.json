{
  "name": "escrow_frontend",
  "version": "0.2.0",
  "description": "Internet Computer starter application",
  "keywords": [
    "Internet Computer",
    "Motoko",
    "JavaScript",
    "Canister"
  ],
  "scripts": {
    "start": "run-p frontend",
    "frontend": "vite --port 3000",
    "backend": "dfx deploy ckhedge_backend",
    "build": "tsc && vite build",
    "deploy:local": "dfx deploy --network=local",
    "deploy:ic": "dfx deploy --network=ic",
    "generate": "dfx generate escrow_backend",
    "setup": "dfx identity use minter && export MINTER_ACCOUNT_ID=$(dfx ledger account-id) && dfx identity use default && export DEFAULT_ACCOUNT_ID=$(dfx ledger account-id)",
    "deploy": "dfx deploy --specified-id ryjl3-tyaaa-aaaaa-aaaba-cai ledger_canister --argument \"(variant { Init = record { minting_account = \\\"$MINTER_ACCOUNT_ID\\\"; initial_values = vec { record { \\\"$DEFAULT_ACCOUNT_ID\\\"; record { e8s = 10_000_000_000 : nat64; }; }; }; send_whitelist = vec {}; transfer_fee = opt record { e8s = 10_000 : nat64; }; token_symbol = opt \\\"LICP\\\"; token_name = opt \\\"Local ICP\\\" } })\" && dfx deploy"
  },
  "dependencies": {
    "@dfinity/agent": "^0.20.2",
    "@dfinity/auth-client": "^0.20.2",
    "@dfinity/candid": "^0.20.2",
    "@dfinity/principal": "^0.20.2",
    "ic-websocket-js": "^0.3.2",
    "postcss-loader": "^7.3.3",
    "process": "^0.11.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.0",
    "react-toastify": "^9.1.3",
    "vite-plugin-top-level-await": "^1.4.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@vitejs/plugin-react": "^4.0.1",
    "autoprefixer": "^10.4.16",
    "bignumber.js": "^9.1.1",
    "cross-fetch": "^3.1.6",
    "dotenv": "^16.3.1",
    "jsdom": "^22.1.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.31",
    "prettier": "^2.8.8",
    "sass": "^1.63.6",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.1.6",
    "vite": "^4.3.9",
    "vite-plugin-environment": "^1.1.3",
    "vitest": "^0.32.2"
  },
  "engines": {
    "node": "^12 || ^14 || ^16 || >=17",
    "npm": "^7.17 || >=8"
  },
  "browserslist": [
    "last 2 chrome version",
    "last 2 firefox version",
    "last 2 safari version",
    "last 2 edge version"
  ]
}
